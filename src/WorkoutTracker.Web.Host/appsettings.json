{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "Database": "ConnectionStrings__Database"
  },

  "Jwt": {
    "Issuer": "Jwt__Issuer",
    "Audience": "Jwt__Audience",
    "SecretKey": "Jwt__SecretKey",
    "ExpiryMinutes": 60
  },

  "Smtp": {
    "Username": "Smtp__Username",
    "Password": "Smtp__Password",
    "Host": "smtp.gmail.com",
    "Port": 587,
    "EnableSsl": true
  }
}
